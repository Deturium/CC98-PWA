{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": { "esmodules": true }
    }],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["transform-imports", {
      "@material-ui/core": { "transform": "@material-ui/core/${member}" },
      "lodash-es": { "transform": "lodash-es/${member}" }
    }],
    ["babel-plugin-styled-components", { "ssr": false }]
  ]
}

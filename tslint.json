{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "tslint-react",
      "eslint-plugin-emotion"
  ],
  "jsRules": {
      "import-name": false
  },
  "rules": {
    "semicolon": false,
    "no-boolean-literal-compare": true,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "member-access": false,
    "jsx-boolean-value": ["never"],
    "syntax-preference": [2, "string"],
    "variable-name": false,
    "import-name": false,
    "ter-arrow-parens": [true, "as-needed"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
